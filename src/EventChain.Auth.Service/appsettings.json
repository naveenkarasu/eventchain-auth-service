{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Authentication": {
    "Google": {
      "ClientId": "${GOOGLE_CLIENT_ID}",
      "ClientSecret": "${GOOGLE_CLIENT_SECRET}"
    },
    "Jwt": {
      "Issuer": "${JWT_ISSUER:-EventChain.Auth.Service}",
      "Audience": "${JWT_AUDIENCE:-EventChain}",
      "SecretKey": "${JWT_SECRET_KEY}",
      "ExpirationMinutes": "${JWT_EXPIRATION_MINUTES:-60}"
    }
  },
  "Frontend": {
    "BaseUrl": "${FRONTEND_BASE_URL:-http://localhost:3001}"
  }
}
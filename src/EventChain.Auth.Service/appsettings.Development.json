{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Authentication": {
    "Google": {
      "ClientId": "YOUR_GOOGLE_CLIENT_ID",
      "ClientSecret": "YOUR_GOOGLE_CLIENT_SECRET"
    },
    "Jwt": {
      "Issuer": "EventChain.Auth.Service",
      "Audience": "EventChain",
      "SecretKey": "YOUR_SUPER_SECRET_KEY_MIN_32_CHARACTERS_LONG_FOR_HS256",
      "ExpirationMinutes": 60
    }
  },
  "Frontend": {
    "BaseUrl": "http://localhost:3001"
  }
}